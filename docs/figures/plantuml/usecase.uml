actor "Subject" as sub
actor "Camera operator" as camop
actor "Image annotators" as imano

package Preprocessing {
    usecase "OpenPose" as openpose
    usecase "Annotations" as ano
}

package System {
    usecase "Dataset"  as data
    usecase "Python prog" as python
    usecase "Statistic data" as stat
    usecase "Position data" as pos
}

sub --> camop
camop --> openpose
camop --> ano
imano --> ano
ano --> data
openpose --> data
data --> python
python --> stat
python --> pos

